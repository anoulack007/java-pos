meta {
  name: login
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "username": "{{adminUsername}}",
    "password": "{{adminPassword}}"
  }
}

script:post-response {
    const data = res.body;
    if (data && data.accessToken) {
      bru.setEnvVar("accessToken", data.accessToken);
    }
}

settings {
  encodeUrl: true
  timeout: 0
}
